SELECT C.CNUM,C.CNAME, S1.SNUM AS SALESPEOPLE_1,S1.SNAME AS SALESPEOPLE_SNAME_1, S2.SNUM AS SALESPEOPLE_2,S2.SNAME AS SALESPEOPLE_NAME_2, S3.SNUM AS SALESPEOPLE_3, S3.SNAME AS SALESPEOPLE_NAME_3
FROM CUST C
JOIN SALESPEOPLE S1
JOIN SALESPEOPLE S2 ON S1.SNUM<S2.SNUM
JOIN SALESPEOPLE S3 ON S2.SNUM<S3.SNUM
ORDER BY C.CNUM;